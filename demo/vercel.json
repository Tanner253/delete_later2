{
  "version": 2,
  "builds": [
    {
      "src": "demo-server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "swagger.json",
          "dist/**",
          "node_modules/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "demo-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

